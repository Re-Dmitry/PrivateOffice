        "resolve-url-loader": "^3.1.0",
        "ts-loader": "^8.0.10",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@types/react": "^16.9.56",
        "@types/react-router-dom": "^5.1.6",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0"
    }
